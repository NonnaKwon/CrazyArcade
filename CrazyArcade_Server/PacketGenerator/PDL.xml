			<int name ="id"/>
			<string name ="roomName"/>
			<int name ="map"/>
			<int name ="playerCount"/>
			<bool name ="isStart"/>
		</list>
	</packet>
	<packet name ="C_CreateRoom">
		<int name="roomId"/>
		<string name="roomName"/>
	</packet>
	<packet name="S_CreateRoom">
